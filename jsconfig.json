{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "resources/*": [
        "resources/*"
      ],
      "js/*": [
        "resources/js/*"
      ],
      "components/*": [
        "resources/js/components/*"
      ],
      "pages/*": [
        "resources/js/pages/*"
      ],
      "api/*": [
        "resources/js/api/*"
      ],
    }
  },
  "include": [
    "./resources/js/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
